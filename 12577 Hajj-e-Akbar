
#include <iostream>
#include<cstdio>
#include<string>



using namespace std;

int main()
{
    int j=1;
    string str;

    while(cin>>str,str!="*")
    {
        if(str=="Hajj") printf("Case %d: Hajj-e-Akbar\n",j);
        else printf("Case %d: Hajj-e-Asghar\n",j);
        j++;

    }

    return 0;

}
