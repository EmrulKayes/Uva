
#include <iostream>
#include<cstdio>


using namespace std;

int main()
{
    int test_case,j=1,H,L,W;

    cin >> test_case;

    while(test_case--)
    {
        scanf("%d %d %d",&L,&W,&H);
        if(L>20 || W>20 || H>20) printf("Case %d: bad\n",j);
        else printf("Case %d: good\n",j);

        j++;

    }

    return 0;

}
