#include <iostream>
#include <cstdio>

using namespace std;

int main(){
    int test_case,event,j=1,emoogle_balance;

    while(cin>>test_case && test_case!=0)
    {
        emoogle_balance=0;

        for(int i=0;i<test_case;i++)
        {
            scanf("%d",&event);
            if(event>0) emoogle_balance++;
            else emoogle_balance--;

        }

       cout<<"Case "<<j<<": "<<emoogle_balance<<endl;
       j++;

    }

        return 0;
}

